<html>
<head>
    <title>My First IIIF Zoom</title>
    <script src="https://cdn.rawgit.com/openseadragon/openseadragon/master/openseadragon.min.js"></script>
</head>
<body>
    <div id="openseadragon1" style="width: 800px; height: 600px;"></div>
    <script type="text/javascript">
        var viewer = OpenSeadragon({
          preserveViewport:   true,
          visibilityRatio:    1,
          minZoomLevel:       1,
          defaultZoomLevel:   1,
            id: "openseadragon1",
            tileSources: [
                ""
            ]
        });
    </script>
</body>
</html>